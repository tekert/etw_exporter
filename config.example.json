{
    "server": {
        "listen_address": ":9189",
        "metrics_path": "/metrics"
    },
    "collectors": {
        "disk_io": {
            "enabled": true,
            "track_disk_info": true
        },
        "thread": {
            "enabled": true,
            "context_switches": true
        }
    },
    "global": {
        "logging": {
            "global": {
                "level": "info",
                "caller": 1,
                "timeField": "time",
                "timeFormat": "",
                "timeLocation": "Local"
            },
            "outputs": [
                {
                    "type": "io",
                    "enabled": true,
                    "config": {
                        "writer": "stderr",
                        "async": false
                    }
                },
                {
                    "type": "console",
                    "enabled": false,
                    "config": {
                        "colorOutput": true,
                        "quoteString": true,
                        "endWithMessage": true,
                        "format": "json",
                        "writer": "stderr",
                        "async": false
                    }
                },
                {
                    "type": "file",
                    "enabled": true,
                    "config": {
                        "filename": "logs/app.log",
                        "fileMode": 644,
                        "maxSize": 104857600,
                        "maxBackups": 7,
                        "timeFormat": "2006-01-02T15-04-05",
                        "localTime": true,
                        "hostName": true,
                        "processID": true,
                        "ensureFolder": true,
                        "async": true
                    }
                },
                {
                    "type": "syslog",
                    "enabled": false,
                    "config": {
                        "network": "udp",
                        "address": "localhost:514",
                        "hostname": "",
                        "tag": "etw_exporter",
                        "marker": "",
                        "async": true
                    }
                },
                {
                    "type": "eventlog",
                    "enabled": false,
                    "config": {
                        "source": "ETW_Exporter",
                        "id": 1000,
                        "host": "",
                        "async": false
                    }
                }
            ],
            "libLevel": "trace"
        }
    }
}